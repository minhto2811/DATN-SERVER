<div
  class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom"
>
  <h1 class="h2">Loại sản phẩm</h1>
  <div class="btn-toolbar mb-2 mb-md 0">
    <div class="btn-group me-2">

    </div>
  </div>
</div>

<div class="col py-3">
  <div class="row">
    <div class="col">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/type">Loại sản phẩm</a></li>
          <li class="breadcrumb-item active">Loại sản phẩm mới</li>
        </ol>
      </nav>
    </div>
    <div class="col text-end fw-lighter">
    </div>
  </div>
</div>

<form action="/type/insert" method="POST"  enctype="multipart/form-data">
  

  <div class="row form-group mb-4">
    <div class="col">
      <label style="margin: 5px;" for="email">Tên loại</label>
      <input
        type="text"
        class="form-control"
        id="name"
        name="name"
        value=""
        placeholder="Tên loại"
        required
      />
    </div>
    
  </div>
 
  <div class="row form-group mb-4">
    <div class="col">
        <label style="margin: 5px;" for="lastName">Hình ảnh</label>
        <input
          type="file"
          class="form-control"
          id="image"
          name="image"
          value=""
          placeholder="Image"
          required
        />
      </div>
  </div>

 

  <div class="form-group mb-4">
    <button type="submit" class="btn btn-primary">Xác nhận</button>
  </div>
</form>
